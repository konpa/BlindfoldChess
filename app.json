{
  "expo": {
    "name": "BlindfoldLichess",
    "slug": "BlindfoldLichess",
    "scheme": "io.konpa.blindfoldlichess",
    "version": "0.0.1",
    "privacy": "unlisted",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "io.konpa.blindfoldlichess",
      "buildNumber": "0.0.1",
      "supportsTablet": true
    },
    "android": {
      "package": "io.konpa.blindfoldlichess",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "startUrl": "https://konpa.github.io/BlindfoldLichess/"
    }
  }
}
